install:: $(PAT)
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(PAT); do \
	(set -x; $(INSTALL) -c -m 555 $$i $(SCRIPTDIR)); \
	done

deinstall::
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(PAT); do \
	(set -x; $(RM) $(SCRIPTDIR)/$$i); \
	done

install.man:: pat.man
